<h1 class="underline">Polideportivos de Madrid</h1>
<div class="container justify-content-center">

  {{#each polisInfo}}
  <h2 id="{{id}}">{{title}}</h2>
  <h3>Dirección</h3>
  <p>{{address.street-address}}, {{address.street-address.postal-code}} {{address.locality}}</p>
  <h3>Servicios</h3>
  <p>{{organization.services}}</p>
  <h3>Horarios</h3>
  <p>{{organization.schedule}}</p>
  <h3>Cómo llegar</h3>
  <p>{{organization.organization-desc}}</p>
  <hr>
  {{/each}}

  <figure id="placesMap"></figure>
</div>
{{!-- Buscar boton para ir arriba --}}
<button id="buttonUp" class="myBtn"><img class="up" src="../../images/up.png" alt="up arrow"></button>

<script src="/javascripts/goUp.js"></script>
<script src="https://unpkg.com/axios/dist/axios.min.js"></script>
<script src="/javascripts/map.styles.js"></script>
<script src="/javascripts/map.configs.js"></script>
<script src="https://maps.googleapis.com/maps/api/js?key={{secret}}&callback=initMap"></script>